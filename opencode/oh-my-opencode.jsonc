{
  "$schema": "https://raw.githubusercontent.com/code-yeongyu/oh-my-opencode/master/assets/oh-my-opencode.schema.json",
  "google_auth": false,
  "agentOverrides": {
    "hephaestus": {
      "model": "wolf-ai/ack-dev",
    },
  },
  "agents": {
    "sisyphus_agent": {
      "disabled": false,
      "default_builder_enabled": true,
      "planner_enabled": false,
      "replace_plan": true,
    },
    "prometheus": {
      "model": "wolf-ai/ack-dev",
      // "variant": "high",
    },
    "sisyphus": {
      "model": "wolf-ai/ack-dev",
      // "variant": "low",
    },
    "sisyphus-junior": {
      "model": "wolf-ai/ack-dev",
    },
    "momus": {
      "model": "google/antigravity-gemini-3-flash",
      "variant": "max",
      // "model": "wolf-ai/ack-dev",
    },
    "metis": {
      // "model": "wolf-ai/ack-dev",
      "model": "antigravity-claude-opus-4-6-thinking",
      "variant": "max",
    },
    "atlas": {
      // "model": "google/antigravity-gemini-3-flash",
      // "variant": "medium",
      // "model": "wolf-ai/k2p5",
      "model": "wolf-ai/ack-dev",
    },
    "oracle": {
      "model": "wolf-ai/ack-dev",
      // "variant": "high",
    },
    "librarian": {
      "model": "wolf-ai/ack-dev",
    },
    "explore": {
      "model": "wolf-ai/ack-dev",
    },
    "document-writer": {
      "model": "wolf-ai/ack-dev",
    },
    "multimodal-looker": {
      "model": "wolf-ai/ack-dev",
      // "variant": "high",
    },
  },
  "categories": {
    "quick": {
      "model": "wolf-ai/ack-dev",
    },
    "unspecified-low": {
      "model": "wolf-ai/ack-dev",
    },
    "unspecified-high": {
      "model": "wolf-ai/ack-dev",
      // "model": "wolf-ai/ack-dev",
      // "variant": "high",
    },
    "writing": {
      "model": "wolf-ai/ack-dev",
    },
    "ultrabrain": {
      // "model": "google/antigravity-gemini-3-pro",
      "model": "wolf-ai/ack-dev",
      // "variant": "high",
    },
    "visual-engineering": {
      "model": "wolf-ai/ack-dev",
      // "variant": "high",
    },
    "deep": {
      "model": "wolf-ai/ack-dev",
      // "model": "google/antigravity-gemini-3-pro",
      // "variant": "high",
    },
    "artistry": {
      "model": "wolf-ai/ack-dev",
      // "variant": "high",
    },
  },
  "background_task": {
    "defaultConcurrency": 3,
    "providerConcurrency": {
      "google": 3,
      "wolf-ai": 20,
    },
  },
  "disabled_hooks": ["comment-checker", "agent-usage-reminder", "keyword-detector"],
  "disabled_agents": [],
  "tmux": {
    "enabled": true,
    "layout": "even-vertical",
    "main_pane_size": 60,
    "main_pane_min_width": 120,
    "agent_pane_min_width": 40,
  },
}
